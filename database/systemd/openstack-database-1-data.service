[Unit]
Description=openstack-database-data

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStartPre=/usr/bin/docker pull dockenstack/base
ExecStart=/bin/sh -c "docker inspect openstack-database-data >/dev/null 2>&1 || docker run --name openstack-database-data -v /var/lib/mysql dockenstack/base true"

[Install]
WantedBy=multi-user.target

#[X-Fleet]
#X-ConditionMachineBootID=CHANGEME

